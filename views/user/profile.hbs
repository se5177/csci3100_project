<h1>Profile</h1>

{{#each data}}
    
    <div class="card p-3">
        <div class="d-flex align-items-center">
            <div class="image"> <img src="{{UserIMG}}" class="rounded" width="155"> </div>
            <div class="ml-3 w-100">
                <h4 class="mb-0 mt-0">{{Name}}</h4> <span></span>
                <div class="p-2 mt-2 bg-primary d-flex justify-content-between rounded text-white stats">
                    <div class="d-flex flex-column"> <span class="Money">Money</span> <span class="number1">{{Money}}</span> </div>
                    <div class="d-flex flex-column"> <span class="No.Product">No.Product</span> <span class="number2">{{No_product}}</span> </div>
                    <div class="d-flex flex-column"> <span class="Uploaded">Uploaded</span> <span class="number3">{{uploaded_product}}</span> </div>
                </div>
               
            </div>
        </div>

 {{/each}} 
</div>

<div class="container">
        <div class="row justify-content-center">
          <div class="col-12 col-sm-8 col-lg-6">
            <!-- Section Heading-->
            <div class="section_heading text-center wow fadeInUp" data-wow-delay="0.2s" style="visibility: visible; animation-delay: 0.2s; animation-name: fadeInUp;">
              <h3>Product </h3>
              
              <div class="line"></div>
            </div>
          </div>
        </div>
        <div class="row">


          {{#each data2}}
          <div class="col-sm-4 col-md-4 col-lg-4">
            <div class="thumbnail">
            <img src="{{this.imagePath}}" alt="..." class="img-responsive">
            <div class="word">
                <div class="caption">
                <h5>  {{this.title}}</h5>
                </div>
            </div>
        </div>
    </div>
          {{/each}}
          <!-- Single Advisor-->


        </div>
      </div>